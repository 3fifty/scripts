{
    "powershell.powerShellDefaultVersion": "PowerShell 7 Core (x64)",
    "powershell.scriptAnalysis.enable": true,
    "powershell.codeFormatting.openBraceOnSameLine": true,
    "powershell.codeFormatting.newLineAfterOpenBrace": false,
    "powershell.codeFormatting.newLineAfterCloseBrace": false,
    "powershell.codeFormatting.pipelineIndentationStyle": "NoIndentation",
    "powershell.codeFormatting.whitespaceBeforeOpenBrace": true,
    "powershell.codeFormatting.whitespaceBeforeOpenParen": true,
    "powershell.codeFormatting.whitespaceAroundOperator": true,
    "powershell.codeFormatting.whitespaceAfterSeparator": false,
    "powershell.codeFormatting.whitespaceInsideBrace": false,
    "powershell.codeFormatting.whitespaceAroundPipe": true,
    "powershell.codeFormatting.ignoreOneLineBlock": true,
    "powershell.codeFormatting.alignPropertyValuePairs": true,
    //Git
    "git.autofetch": true,
    //Files
    "files.defaultLanguage": "powershell",
    "files.encoding": "utf8",
    //Editor
    "editor.foldingStrategy": "indentation",
    "editor.formatOnSave": true,
    "editor.formatOnSaveTimeout": 1000,
    //Telemetry
    "telemetry.enableTelemetry": false,
    //cSpell Dictionary
    "cSpell.words": [
        "ADAPI",
        "ARMTTK",
        "CMMI",
        "IDURI",
        "KPMG",
        "Managementgroup",
        "Pathto",
        "Resourcegroup",
        "Templatefile",
        "aaduserobjectid",
        "aaduserpassword",
        "armttk",
        "costlimit",
        "deadletter",
        "devops",
        "erroritem",
        "hashtable",
        "issecret",
        "jsonbody",
        "logicapp",
        "logissue",
        "msal",
        "newsvcprincipal",
        "nuget",
        "powershellmodule",
        "psmodules",
        "pwsh",
        "rbac",
        "replacetokens",
        "securefile",
        "securestring",
        "servicebus",
        "serviceprincipal",
        "setvariable",
        "statuscode",
        "supersecretkey",
        "targetscope",
        "templatelocation",
        "topic",
        "userentitlements",
        "versioncontrol",
        "versioncounter",
        "versionnumber",
        "westeurope"
    ],
    "cSpell.ignoreWords": [
    ],
    "powershell.codeFormatting.addWhitespaceAroundPipe": true
}